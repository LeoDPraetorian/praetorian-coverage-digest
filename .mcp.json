{
  "mcpServers": {
    "chariot": {
      "type": "stdio",
      "command": "go",
      "args": [
        "run",
        "./modules/chariot/backend/cmd/tools/chariot-mcp"
      ]
    },
    "praetorian-cli": {
      "type": "stdio",
      "command": "praetorian",
      "args": [
        "--profile",
        "demo",
        "chariot",
        "agent",
        "mcp",
        "start"
      ]
    },
    "nebula": {
      "type": "stdio",
      "command": "go",
      "args": [
        "run",
        "./modules/nebula/cmd/mcp-server.go"
      ]
    },
    "atlassian": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.atlassian.com/v1/sse"
      ]
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--isolated"
      ],
      "env": {}
    }
  }
}
