{
  "mcpServers": {
    "atlassian": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.atlassian.com/v1/sse"]
    },
    "chariot": {
      "type": "stdio",
      "command": "go",
      "args": ["run", "./modules/chariot/backend/cmd/tools/chariot-mcp"]
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": ["chrome-devtools-mcp@latest"]
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@latest"],
      "env": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      }
    },
    "linear": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.linear.app/sse"]
    },
    "nebula": {
      "type": "stdio",
      "command": "go",
      "args": ["run", "./modules/nebula/cmd/mcp-server.go"]
    },
    "praetorian-cli": {
      "type": "stdio",
      "command": "praetorian",
      "args": ["--profile", "demo", "chariot", "agent", "mcp", "start"]
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["@playwright/mcp@latest", "--isolated"],
      "env": {}
    },
    "currents": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@currents/mcp"],
      "env": {
        "CURRENTS_API_KEY": "${CURRENTS_API_KEY}"
      }
    }
  }
}
